{
  "scopeName": "source.sql.browserslist",
  "name": "Browserslist",
  "fileTypes": ["browserslistrc"],
  "patterns": [
    {
      "include": "#comment"
    },
    {
      "include": "#countries"
    },
    {
      "include": "#configs"
    },
    {
      "include": "#dates"
    },
    {
      "include": "#environments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#sections"
    },
    {
      "include": "#versions"
    }
  ],
  "repository": {
    "comment": {
      "name": "comment.line.browserslist",
      "begin": "#",
      "end": "\n"
    },
    "configs": {
      "patterns": [
        {
          "name": "entity.name.class.browserslist",
          "match": "@\\w+\\/browserslist-config(-.+)?"
        },
        {
          "name": "entity.name.class.browserslist",
          "match": "\\bbrowserslist-config-\\w+\\b"
        }
      ]
    },
    "countries": {
      "patterns": [
        {
          "name": "support.type.country.browserslist",
          "match": "\\b(AD|AE|AF|AG|AI|AL|AM|AN|AO|AR|AS|AT|AU|AW|AX|AZ|BA|BB|BD|BE|BF|BG|BH|BI|BJ|BM|BN|BO|BR|BS|BT|BW|BY|BZ|CA|CD|CF|CG|CH|CI|CK|CL|CM|CN|CO|CR|CU|CV|CX|CY|CZ|DE|DJ|DK|DM|DO|DZ|EC|EE|EG|ER|ES|ET|FI|FJ|FK|FM|FO|FR|GA|GB|GD|GE|GF|GG|GH|GI|GL|GM|GN|GP|GQ|GR|GT|GU|GW|GY|HK|HN|HR|HT|HU|ID|IE|IL|IM|IN|IQ|IR|IS|IT|JE|JM|JO|JP|KE|KG|KH|KI|KM|KN|KP|KR|KW|KY|KZ|LA|LB|LC|LI|LK|LR|LS|LT|LU|LV|LY|MA|MC|MD|ME|MG|MH|MK|ML|MM|MN|MO|MP|MQ|MR|MS|MT|MU|MV|MW|MX|MY|MZ|NA|NC|NE|NF|NG|NI|NL|NO|NP|NR|NU|NZ|OM|PA|PE|PF|PG|PH|PK|PL|PM|PN|PR|PS|PT|PW|PY|QA|RE|RO|RS|RU|RW|SA|SB|SC|SD|SE|SG|SH|SI|SK|SL|SM|SN|SO|SR|ST|SV|SY|SZ|TC|TD|TG|TH|TJ|TK|TL|TM|TN|TO|TR|TT|TV|TW|TZ|UA|UG|US|UY|UZ|VA|VC|VE|VG|VI|VN|VU|WF|WS|YE|YT|ZA|ZM|ZW|alt-af|alt-an|alt-as|alt-eu|alt-na|alt-oc|alt-sa|alt-ww)\\b"
        }
      ]
    },
    "dates": {
      "patterns": [
        {
          "name": "constant.numeric.date.browserslist",
          "match": "years?"
        }
      ]
    },
    "environments": {
      "patterns": [
        {
          "name": "support.type.browser.browserslist",
          "match": "(?i)\\b(Android|Baidu|BlackBerry|bb|Chrome|ChromeAndroid|and_chr|Edge|Explorer|ie|ExplorerMobile|ie_mob|Firefox|ff|FirefoxAndroid|and_ff|iOS|ios_saf|Opera|OperaMini|op_mini|OperaMobile|op_mob|PhantomJS|QQAndroid|and_qq|Safari|Samsung|UCAndroid|and_uc|kaios)\\b"
        },
        {
          "name": "support.type.environment.browserslist",
          "match": "(?i)\\b(Electron|Node)\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.operator.browserslist",
          "match": "(<=?|>=?|,)"
        },
        {
          "name": "keyword.control.keyword.browserslist",
          "match": "\\b(and|cover|extends|in|last|not|or|since|supports)\\b"
        }
      ]
    },
    "sections": {
      "patterns": [
        {
          "name": "entity.name.function.browserslist",
          "match": "defaults"
        },
        {
          "name": "entity.name.function.browserslist",
          "match": "(\\[|\\])"
        },
        {
          "name": "entity.name.function.browserslist",
          "match": "(?!\\[)(.+)(?=\\])"
        }
      ]
    },
    "versions": {
      "patterns": [
        {
          "name": "constant.numeric.browserslist",
          "match": "\\b\\d+(.\\d+){0,2}\\b"
        },
        {
          "name": "constant.numeric.divider.browserslist",
          "match": "(\\d*\\.)?\\d"
        },
        {
          "name": "constant.numeric.keyword.browserslist",
          "match": "(?i)\\b(esr|tp|all|dead|unreleased|major|versions?)\\b"
        },
        {
          "name": "constant.numeric.marketshares.browserslist",
          "match": "%"
        },
        {
          "name": "constant.numeric.node.browserslist",
          "match": "(?i)\\b(current|maintained?)\\b"
        }
      ]
    }
  }
}
